                                        "mandatory": [
                                            "option"
                                        ],
                                        "optional": [
                                            "caps_lock"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "zsh -c 'if /usr/bin/pgrep -x \"wezterm-gui\" > /dev/null; then /opt/homebrew/bin/wezterm cli spawn --new-window; else open -a wezterm; fi'"
