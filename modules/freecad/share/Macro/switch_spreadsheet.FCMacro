import FreeCADGui

def switch_to_spreadsheet():
    """Change le workbench actif vers Spreadsheet."""
    try:
        FreeCADGui.activateWorkbench("SpreadsheetWorkbench")
        print("Workbench chang√© vers Spreadsheet.")
    except Exception as e:
        print(f"Erreur : {e}")

switch_to_spreadsheet()
